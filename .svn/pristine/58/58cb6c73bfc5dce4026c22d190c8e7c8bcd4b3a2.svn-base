package com.mochasoft.second.model;

import java.io.Serializable;
import java.util.Date;

public class ReplyMessage implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 2976944567385468237L;

	private String replyId;//回复id
	private String replyContent;//回复内容
	private String dealId;// 交易id
	private String createUser;//回复创建人id
	private Date createTime;// 回复创建时间
	private int status;//状态
	private String replyName;	//回复人用户名
	private String nickname;	//回复人昵称
	private String replyImage;	//回复人头像
	
	public String getReplyId() {
		return replyId;
	}
	public void setReplyId(String replyId) {
		this.replyId = replyId;
	}
	public String getReplyContent() {
		return replyContent;
	}
	public void setReplyContent(String replyContent) {
		this.replyContent = replyContent;
	}
	public String getDealId() {
		return dealId;
	}
	public void setDealId(String dealId) {
		this.dealId = dealId;
	}
	public String getCreateUser() {
		return createUser;
	}
	public void setCreateUser(String createUser) {
		this.createUser = createUser;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public String getReplyName() {
		return replyName;
	}
	public void setReplyName(String replyName) {
		this.replyName = replyName;
	}
	public String getNickname() {
		return nickname;
	}
	public void setNickname(String nickname) {
		this.nickname = nickname;
	}
	public String getReplyImage() {
		return replyImage;
	}
	public void setReplyImage(String replyImage) {
		this.replyImage = replyImage;
	}

	

}
